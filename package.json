        "supabase": "^1.187.3"
    },
    "devDependencies": {
        "@types/node": "20.14.12",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4"
    }
}